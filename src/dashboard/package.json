    "cron-parser": "^2.15.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-fileupload": "^1.1.6",
    "express-pino-logger": "^5.0.0",
    "express-session": "^1.17.1",
    "express-winston": "^4.0.3",
    "gifsicle": "^5.1.0",
    "ip": "^1.1.5",
    "isomorphic-fetch": "^2.2.1",
    "lodash": ">=4.17.19",
    "memorystore": "^1.6.2",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.28",
    "mongoose": "^5.9.10",
    "morgan": "^1.10.0",
