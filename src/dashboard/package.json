{
  "name": "psic_node_server",
  "version": "1.0.0",
  "description": "PSIC Node server",
  "main": "server.js",
  "scripts": {
    "buildSite": "next build ./_site",
    "buildTSC": "tsc --p ./tsconfig.json",
    "build": "npm run buildSite && npm run buildTSC",
    "start": "NODE_ENV=production node ./_dist/server.js",
    "startW": "SET NODE_ENV=production&& node ./_dist/server.js",
    "bas": "npm run build && npm run start",
    "basW": "npm run build && npm run startW",
    "nodemonTSC": "nodemon --exec ts-node-script ./_server/server.ts",
    "dev2": "ts-node-script ./_server/server.ts",
    "dev": "npm run nodemonTSC",
    "test": "nodemon --exec ts-node-script ./_server/test_server.ts",
    "baf": "sudo npm run build && forever stopall && bash forever_start.sh"
  },
  "author": "Shah Nafis Rafique",
  "license": "MIT",
  "dependencies": {
    "@material-ui/core": "^4.9.11",
    "@material-ui/icons": "^4.9.1",
