{
  "name": "uncg-daisy-labeller-dashboard",
  "author": "UNCG-DAISY",
  "license": "MIT",
  "version": "3.6.1",
  "scripts": {
    "dev": "npm run build:server && node dist/server/index.js",
    "build:server": "rimraf dist && tsc --project tsconfig.server.json",
    "build:next": "next build",
    "build": "npm run build:next && npm run build:server",
    "start": "npm run build && npm run prod",
    "prod": "cross-env NODE_ENV=production node dist/server/index.js",
    "type-check": "tsc --pretty --noEmit",
    "format": "prettier --write \"**/*.{js,ts,tsx}\"",
    "lint": "eslint . --ext ts --ext tsx --ext js ",
    "lint:fix": "eslint . --ext ts --ext tsx --ext js --fix",
    "seeder": "cross-env NODE_ENV=test ts-node --project tsconfig.server.json server/seeder.ts",
    "sandbox": "npm run build:server && node dist/server/sandbox.js",
    "jest": "jest",
    "test": "npm run seeder && npm run jest",
    "test-all": "npm run lint && npm run type-check && npm run test",
    "testFile": "ts-node --project tsconfig.server.json tests/index.ts",
    "pm2": "npm run build && pm2 start ecosystem.config.js",
    "pm2:restart": "pm2 stop all && pm2 delete all && npm run pm2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npx lint-staged",
      "pre-push": "npm run type-check"
    }
  },
  "lint-staged": {
    "*.@(ts|tsx)": [
      "npm run lint",
      "npm run format"
    ]
  },
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/data-grid": "^4.0.0-alpha.17",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "archiver": "^5.0.2",
    "colors": "^1.4.0",
    "cron-parser": "^2.16.3",
    "csv-stringify": "^5.5.1",
    "csv-writer": "^1.6.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "express-validator": "^6.6.1",
    "jest-express": "^1.12.0",
    "jsonschema": "^1.2.6",
    "lodash": "^4.17.21",
    "memorystore": "^1.6.2",
    "module-alias": "^2.2.2",
    "moment": "^2.27.0",
    "mongoose": "^5.9.24",
    "next": "9.5.4",
    "node-schedule": "^1.3.2",
    "passport": "^0.4.1",
    "passport-auth0": "^1.3.3",
    "react": "^16.13.1",
    "react-canvas-draw": "^1.1.1",
    "react-cool-img": "^1.2.4",
    "react-csv": "^2.0.3",
    "react-dom": "^16.13.1",
    "react-hook-form": "^6.0.6",
    "react-hooks-svgdrawing": "^2.1.2",
    "react-json-pretty": "^2.2.0",
    "response-time": "^2.3.2",
    "rimraf": "^3.0.2",
    "svg.js": "^2.7.1",
    "svgdom": "^0.1.8",
    "uid-safe": "^2.1.5"
  },
  "devDependencies": {
    "@testing-library/react": "^10.0.1",
    "@types/express": "^4.17.7",
    "@types/express-session": "^1.17.0",
    "@types/jest": "^25.1.4",
    "@types/lodash": "^4.14.157",
    "@types/mongodb": "^3.5.25",
    "@types/mongoose": "^5.7.31",
    "@types/node": "^13.9.5",
    "@types/passport": "^1.0.4",
    "@types/passport-auth0": "^1.0.4",
    "@types/react": "^16.9.27",
    "@types/react-dom": "^16.9.8",
    "@types/testing-library__react": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^2.25.0",
    "@typescript-eslint/parser": "^2.25.0",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-react": "^7.19.0",
    "husky": "^4.2.5",
    "jest": "^25.2.3",
    "jest-watch-typeahead": "^0.5.0",
    "lint-staged": "^10.0.10",
    "node-mocks-http": "^1.8.1",
    "prettier": "^2.0.2",
    "ts-jest": "^26.1.2",
    "ts-node": "^8.10.2",
    "typescript": "3.8.3"
  },
  "_moduleAliases": {
    "@/server": "dist/server",
    "@/models": "dist/server/models",
    "@/utils": "dist/server/utils",
    "@/routes": "dist/server/routes",
    "@/middlewares": "dist/server/middlewares",
    "@/controllers": "dist/server/controllers",
    "@/data": "data",
    "@/interfaces/*": "interfaces"
  }
}
